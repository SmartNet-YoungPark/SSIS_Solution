        {
            DialogResult answer = MessageBox.Show("Choose Excel?", 
                "Choice",
                MessageBoxButtons.YesNo, 
                MessageBoxIcon.Question, MessageBoxDefaultButton.Button2 );

            switch(answer)
            {
                case DialogResult.Yes:
                    
                    Dts.TaskResult = (int)ScriptResults.Success;

